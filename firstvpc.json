{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "First JSON",
    "Parameters":{
          "vpcid": {
              "Description": "Provide VPC-ID:",
              "Type": "String",
              "Default": "-"
          },          
          "subnetid": {
              "Description": "Enter Public Subnet ID:",
              "Type": "String",
              "Default": "-"
          },          
          "privatesubnet": {
              "Description": "Enter Private Subnet ID:",
              "Type": "String",
              "Default": "-"
          }
         },
    "Resources":{
        
        "firstvpc": {
          "Type": "AWS::EC2::VPC",
          "Properties": {
            "CidrBlock": { "Ref": "vpcid" }
            
          }
        },
        
        "publicsubnet": {
          "Type": "AWS::EC2::Subnet",
          "Properties": {
            "VpcId": 
            { "Ref": "firstvpc" }
            ,
            "CidrBlock": 
            { "Ref": "subnetid" }
            ,
            "Tags": [{"Key": "Name","Value": "Public-Subnet"}]
          }
        },
        
        "privatesubnet": {
          "Type": "AWS::EC2::Subnet",
          "Properties": {
            "AvailabilityZone": "-",
            "VpcId": 
            { "Ref": "firstvpc" }
            ,
            "CidrBlock": 
            { "Ref": "privatesubnet" }
            ,
            "Tags": [{"Key":"Name","Value":"Private-Subnet"}]
          }
        },
        
        "igw": {
          "Type": "AWS::EC2::InternetGateway",
          "Properties": {
            "Tags": [{"Key":"Name","Value":"CF-Gateway"}]
          }
        },
        "AttachGateway": {
          "Type": "AWS::EC2::VPCGatewayAttachment",
          "Properties": {
            "VpcId": 
            { "Ref": "firstvpc" }
            ,
            "InternetGatewayId": { "Ref": "igw" }
          }
        },
        
        "routetb": {
          "Type": "AWS::EC2::RouteTable",
          "Properties": {
            "VpcId": 
            { "Ref": "firstvpc" }
            ,
            "Tags": [{"Key":"Name","Value":"CF-Route Table"}]
          }
        },
                
        "route": {
          "Type": "AWS::EC2::Route",
          "Properties": {
            "RouteTableId": 
            { "Ref": "routetb" }
            ,
            "DestinationCidrBlock": "0.0.0.0/0",
            "GatewayId": 
            { "Ref": "igw" }
            
          }
        },
        
        "routeTableAssocName": {
          "Type": "AWS::EC2::SubnetRouteTableAssociation",
          "Properties": {
            "SubnetId": 
            { "Ref": "publicsubnet" }
            ,
            "RouteTableId": "routetb"
          }
        }                     
    }            
}