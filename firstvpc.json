{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "First JSON",
    "Parameters":{
          "vpcid": {
              "Description": "Provide VPC-ID:",
              "Type": "String",
              "Default": "10.25.0.0/16",
              "AllowedValues":["10.25.0.0/16","10.26.0.0/16","10.27.0.0/16"]
          },          
          "publicsubnetaz": {
              "Description": "Availability Subnet AZ:",
              "Type": "AWS::EC2::AvailabilityZone::Name"
          },          
          "privatesubnetaz": {
              "Description": "Private Subnet AZ:",
              "Type": "AWS::EC2::AvailabilityZone::Name"
          },      
          "subnetid": {
              "Description": "Enter Public Subnet ID:",
              "Type": "String",
              "Default": "-"
          },          
          "subnet2": {
              "Description": "Enter Private Subnet ID:",
              "Type": "String",
              "Default": "-"
          }
         },
    "Resources":{
        "firstvpc": {
          "Type": "AWS::EC2::VPC",
          "Properties": {
            "CidrBlock": { "Ref": "vpcid" }  
          }
        },        
        "publicsubnet": {
          "Type": "AWS::EC2::Subnet",
          "Properties": {
            "VpcId": { "Ref": "firstvpc" },
            "AvailabilityZone": "publicsubnetaz",
            "CidrBlock":{ "Ref": "subnetid" },
            "Tags": [{"Key": "Name","Value": "Public-Subnet"}]
          }
        },        
        "privatesubnet": {
          "Type": "AWS::EC2::Subnet",
          "Properties": {
            "AvailabilityZone": "privatesubnetaz",
            "VpcId":{ "Ref": "firstvpc" },
            "CidrBlock":{ "Ref": "subnet2" },
            "Tags": [{"Key":"Name","Value":"Private-Subnet"}]
          }
        },
        "igw": {
          "Type": "AWS::EC2::InternetGateway",
          "Properties": {
            "Tags": [{"Key":"Name","Value":"CF-Gateway"}]
          }
        },
        "AttachGateway": {
          "Type": "AWS::EC2::VPCGatewayAttachment",
          "Properties": {
            "VpcId":{ "Ref": "firstvpc" },
            "InternetGatewayId": { "Ref": "igw" }
          }
        },        
        "routetb": {
          "Type": "AWS::EC2::RouteTable",
          "Properties": {
            "VpcId":{ "Ref": "firstvpc" },
            "Tags": [{"Key":"Name","Value":"CF-Route Table"}]
          }
        },                
        "route": {
          "Type": "AWS::EC2::Route",
          "Properties": {
            "RouteTableId":{ "Ref": "routetb" },
            "DestinationCidrBlock": "0.0.0.0/0",
            "GatewayId":{ "Ref": "igw" }            
          }
        },        
        "routeTableAssocName": {
          "Type": "AWS::EC2::SubnetRouteTableAssociation",
          "Properties": {
            "SubnetId":{ "Ref": "publicsubnet" },
            "RouteTableId": "routetb"
          }
        },
        "apache-sec" : {
          "Type" : "AWS::EC2::SecurityGroup",
          "Properties" : {
            "GroupDescription" : "-",
            "SecurityGroupIngress" : [
            { "IpProtocol" : "tcp", "FromPort" : "0", "ToPort" : "65535", "CidrIp" : "0.0.0.0/0" }
            ],
            "VpcId" : 
            { "Ref": "firstvpc" }
            ,
            "Tags" :  [ {"Key": "Name","Value":"Apache-web"} ]
          }
        }     
                             
    }            
}